@model string
<partial name="_ViewHeader" />
<div class="px-5">

<div class="bg-primary panel-body"><h4>إضافة دور</h4></div>
<div asp-validation-summary="All" class="text-danger"></div>
<form asp-action="Create" method="post">
    <div class="mb-3">
        <label for="name"></label>
        <input name="name" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary rounded-0">إضافة</button>
    <a asp-action="Index" class="btn btn-secondary rounded-0">إلغاء</a>
</form>
</div>